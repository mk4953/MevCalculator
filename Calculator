namespace MevCalculator
{
    public class Math
    {


        public int Add(int a, int b)
        {
            return a + b;
        }

        public uint Sub(uint a, uint b)
        {
            return a - b;
        }

        public long Multiply(long a, long b)
        {
            return a * b;
        }

        public ulong Div(ulong a, ulong b)
        {
            return a / b;
        }
		
	public long Powered(long number, long exponent)
	{
            var temp = number;
            for (int i = 0; i < exponent-1; i++)
            {
                number = number * temp;
            }
	    
            return number;
        }

    }
}
